{
  "name": "@tangible/roller",
  "version": "1.2.4",
  "description": "Build project assets using Rollup and ESBuild",
  "homepage": "https://github.com/tangibleinc/tangible-roller",
  "repository": "git@github.com:tangibleinc/tangible-roller.git",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "roll": "./run"
  },
  "scripts": {
    "release": "npm publish --access public",
    "test": "cd test && if [ ! -d node_modules ]; then npm install; fi && ../run",
    "test:dev": "cd test && ../run dev",
    "test:build": "cd test && ../run build"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "autoprefixer": "^10.3.6",
    "chokidar": "^3.5.2",
    "esbuild": "^0.13.3",
    "eta": "^1.12.3",
    "get-port": "^5.1.1",
    "glob": "^7.2.0",
    "nodemon": "^2.0.13",
    "postcss": "^8.3.8",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-styles": "^3.14.1",
    "sass": "^1.42.1",
    "serve-handler": "^6.1.3"
  }
}
