{
  "name": "@tangible/roller",
  "description": "Next-generation build tool using Rollup and ESBuild",
  "version": "1.1.4",
  "main": "index.js",
  "bin": {
    "roll": "./run"
  },
  "scripts": {
    "release": "npm publish --access public",
    "test": "cd test && if [ ! -d node_modules ]; then npm install; fi && ../run",
    "test:dev": "cd test && ../run dev",
    "test:build": "cd test && ../run build"
  },
  "homepage": "https://github.com/tangibleinc/tangible-roller",
  "repository": "git@github.com:tangibleinc/tangible-roller.git",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "autoprefixer": "^10.2.6",
    "chokidar": "^3.5.2",
    "esbuild": "^0.12.9",
    "eta": "^1.12.3",
    "get-port": "^5.1.1",
    "glob": "^7.1.7",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.5",
    "rollup": "^2.52.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-polyfill-node": "^0.6.2",
    "rollup-plugin-styles": "^3.14.1",
    "sass": "^1.35.1",
    "serve-handler": "^6.1.3"
  }
}
