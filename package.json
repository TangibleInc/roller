{
  "name": "@tangible/roller",
  "version": "1.7.1",
  "description": "Build project assets using Rollup and ESBuild",
  "homepage": "https://github.com/tangibleinc/tangible-roller",
  "repository": "git@github.com:tangibleinc/tangible-roller.git",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "roll": "./run"
  },
  "tangible": {
    "deployName": "tangible-roller",
    "deployType": "tool"
  },
  "scripts": {
    "release": "npm publish --access public",
    "test": "cd test && if [ ! -d node_modules ]; then npm install; fi && ../run",
    "test:dev": "cd test && ../run dev",
    "test:build": "cd test && ../run build"
  },
  "dependencies": {
    "@ironkinoko/rollup-plugin-styles": "^4.0.3",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "esbuild": "^0.17.18",
    "eta": "^3.1.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "rollup": "^3.21.5",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-external-globals": "^0.7.3",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "sass": "^1.62.1",
    "serve-handler": "^6.1.5",
    "ws": "^8.13.0"
  }
}
