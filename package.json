{
  "name": "@tangible/roller",
  "version": "1.5.6",
  "description": "Build project assets using Rollup and ESBuild",
  "homepage": "https://github.com/tangibleinc/tangible-roller",
  "repository": "git@github.com:tangibleinc/tangible-roller.git",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "roll": "./run"
  },
  "tangible": {
    "deployName": "tangible-roller",
    "deployType": "tool"
  },
  "scripts": {
    "release": "npm publish --access public",
    "test": "cd test && if [ ! -d node_modules ]; then npm install; fi && ../run",
    "test:dev": "cd test && ../run dev",
    "test:build": "cd test && ../run build"
  },
  "dependencies": {
    "@ironkinoko/rollup-plugin-styles": "^4.0.3",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.4",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.5",
    "eta": "^3.1.1",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-external-globals": "^0.8.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "sass": "^1.69.4",
    "serve-handler": "^6.1.5",
    "ws": "^8.14.2"
  }
}
